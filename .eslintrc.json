{
    "extends": "./node_modules/eslint-config-google/index.js",
    
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["extra-rules"],   
    "rules": {
        "semi": 2,
        "no-invalid-this": 0,
        "no-var": "warn",
        "max-len": 0,
        "camelcase": "warn",
        "no-trailing-spaces": ["warn", {"skipBlankLines":true}],
        "extra-rules/no-commented-out-code": "warn"
    }

}
